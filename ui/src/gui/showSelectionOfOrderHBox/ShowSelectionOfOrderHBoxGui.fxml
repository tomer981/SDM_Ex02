<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.showSelectionOfOrderHBox.ShowSelectionOfOrderHBoxController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Show Order:" />
      <ComboBox fx:id="orderComboBox" prefWidth="150.0" promptText="Order Id" />
      <Button fx:id="showOrderButton" mnemonicParsing="false" text="Show Order" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
