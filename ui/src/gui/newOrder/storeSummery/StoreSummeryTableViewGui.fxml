<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>


<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.newOrder.storeSummery.StoreSummeryTableViewController">
   <content>
      <TableView fx:id="storeTableView" minHeight="110.0" minWidth="330.0">
        <columns>
          <TableColumn maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="Store">
               <columns>
                <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
                  <TableColumn fx:id="name" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="ppk" prefWidth="75.0" text="PPK" />
                  <TableColumn fx:id="distanceCustomer" prefWidth="75.0" text="Distance&#10;Customer" />
                  <TableColumn fx:id="deliveryCost" prefWidth="75.0" text="Delivery&#10;Cost" />
               </columns>
            </TableColumn>
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </content>
</ScrollPane>
