<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="boarderPaneLayout" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.newOrder.dynamicLayoutBoarderPane.DynamicLayoutBoarderPaneController">
   <top>
      <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="50.0" BorderPane.alignment="CENTER">
         <children>
            <ComboBox fx:id="storeComboBox" onAction="#OnStoreSelectionComboBox" prefWidth="150.0" promptText="Store Selection" />
         </children>
         <padding>
            <Insets left="15.0" />
         </padding>
      </HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#onNextButton" text="Next" />
         </children>
         <padding>
            <Insets right="20.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
