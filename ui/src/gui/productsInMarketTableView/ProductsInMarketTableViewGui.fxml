<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.productsInMarketTableView.ProductsInMarketTableViewController">
   <content>
      <TableView fx:id="MarketProductsTableView" editable="true" minHeight="150.0" minWidth="450.0">
         <columns>
            <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Products In Market">
               <columns>
                  <TableColumn fx:id="idTableColumn" prefWidth="75.0" text="ID" />
                  <TableColumn fx:id="nameTableColumn" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="categoryTableColumn" prefWidth="75.0" text="Category" />
                  <TableColumn fx:id="storeSellTableColumn" prefWidth="75.0" text="Store Sell" />
                  <TableColumn fx:id="avgPriceTableColumn" prefWidth="75.0" text="Avg Price" />
                  <TableColumn fx:id="timeSoldTableColumn" prefWidth="75.0" text="Time Sold" />
               </columns>
            </TableColumn>
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </TableView>
   </content>
</ScrollPane>
