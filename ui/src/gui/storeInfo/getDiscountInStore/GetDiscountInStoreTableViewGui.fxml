<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.storeInfo.getDiscountInStore.GetDiscountInStoreTableViewController">
   <content>
      <TableView fx:id="getTableView" minHeight="120.0" minWidth="300.0">
        <columns>
            <TableColumn fx:id="typeOrder" prefWidth="75.0" text="One-Of / All-Or-Nothing">
               <columns>
                  <TableColumn prefWidth="75.0" text="Get">
                     <columns>
                        <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
                        <TableColumn fx:id="name" prefWidth="75.0" text="Name" />
                     </columns>
                  </TableColumn>
                  <TableColumn fx:id="amount" prefWidth="75.0" text="Amount" />
                <TableColumn fx:id="price" prefWidth="75.0" text="Price" />
               </columns>
            </TableColumn>
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </content>
</ScrollPane>
